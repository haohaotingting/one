@import "../../css/iconfont.wxss";

/* 整个民宿 */
.minsutupina{
  display:flex;
  height:220px;
  /* background:lightcyan; */
  padding:15px 0px 8px 0px;
}
.minsutupian1{
  flex:1;
  /* background:red; */
  padding-left:8px;
}
.minsutupian2{
  flex:1;
  /* background:green; */
  padding-right:8px;
}

/* 民宿标题头 */
.mins{
  height:18%;
  /* background:red; */
}
.minsu{
  height:18%;
  /* background:green; */
  text-align:right;
}
.gengduo{
  font-size:12px;
  text-align:right;
  color:rgb(138, 136, 136);
  padding-right:3px;
}
 .min{
  font-size:18px;
  color:#fff;
  border-radius:50%;
  padding:5px 8px;
  background:rgb(194, 150, 29);
}
.mins .su{
  padding-left:3px;
} 


.image1,.image2{
  width:96%;
  height:82%;
  margin:0 auto;
  box-shadow: 5px 5px  6px #888888;
  /* background:red; */
}

.imagea,.imageb{
  width:100%;
  height:80%;
  position:relative;
}
.imagea image,.imageb image{
  width:100%;
  height:100%;
  border-radius:5px 5px 0px 0px;
}
.yidong1,.yidong2{
  position:absolute;
  left: 0px;
  bottom: 0px;
  background:rgba(33, 33, 34, 0.568);
  width:100%;
}
.yanyu1,.yanyu2{
  padding-left:10px;
  color:#fff;
  font-size:14px;
}

.duoshaoqian1,.duoshaoqian2{
  height:20%;
  background:#fff;
  box-shadow: 0px 3px  0px rgb(131, 129, 129);
}
.haoduoqian1,.haoduoqian2{
  padding:5px 0px 0px 10px;
  font-size:14px;
 
}
.icon-renminbi1688{
  color:red;
  font-weight:600;
  font-size:12px;
}
.qianqian1,.qianqian2{
  color:red;
}
.qimeiwan1,.qimeiwan2{
  color:rgba(33, 33, 34, 0.568);
}

